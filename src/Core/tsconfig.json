{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "es6",
	  "lib": ["ES2020"],
	  "outDir": "dist",
	  "rootDir": "",
	  "sourceMap": true,
	  "strictPropertyInitialization": false,
	  "declaration": true,
	  "resolveJsonModule": true,
	  "skipLibCheck": true,
	  "experimentalDecorators": true,
	  "esModuleInterop": true
	},
	"exclude": ["node_modules", ".vscode-test", "_code-dep"],
}