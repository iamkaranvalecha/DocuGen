{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "es6",
	  "outDir": "./dist",
	  "rootDir": "src",
	  "sourceMap": true,
	  "strictPropertyInitialization": false,
	  "declaration": true,
	  "resolveJsonModule": true,
	  "skipLibCheck": true,
	  "experimentalDecorators": true,
	  "esModuleInterop": true
	},
	"include": ["src"],
	"exclude": ["./node_modules/@types","node_modules", ".vscode-test", "_code-dep"],
}