{
  "name": "docugen",
  "displayName": "DocuGen",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "docugen.scanRepository",
        "title": "Generate Document by scanning repository"
      }
    ],
    "configuration": [
      {
        "title": "DocuGen",
        "scope": "resource",
        "properties": {
          "docugen.modelProvider": {
            "type": "string",
            "enum": [
              "AzureOpenAI",
              "Ollama"
            ],
            "description": "Select a model provider from the pre-defined list",
            "default": "AzureOpenAI",
            "title": "Model Provider",
            "workspace": true,
            "scope": "resource"
          },
          "docugen.modelName": {
            "type": "string",
            "description": "Model name. (example: gpt-4o)",
            "default": "gpt-4o",
            "title": "Model Name",
            "workspace": true,
            "scope": "resource"
          },
          "docugen.modelEndpoint": {
            "type": "string",
            "description": "Azure OpenAI model endpoint. (example: https://<instance>.openai.azure.com)",
            "default": "https://vsmodel.openai.azure.com",
            "title": "Model Endpoint",
            "workspace": true,
            "scope": "resource"
          },
          "docugen.modelApiVersion": {
            "type": "string",
            "description": "Model API Version (example: 2023-03-15-preview)",
            "default": "2023-03-15-preview",
            "title": "API version",
            "workspace": true,
            "scope": "resource"
          },
          "docugen.modelApiKey": {
            "type": "string",
            "description": "Model API key",
            "default": "",
            "title": "API key",
            "workspace": true,
            "scope": "resource"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "problemMatcher": "$tsc",
    "postinstall": "node ../install-deps.js"
  },
  "devDependencies": {
    "@types/estree": "^1.0.6",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "docugen": "file:../../Core"
  }
}
